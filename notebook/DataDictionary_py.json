					"class DataDictionary(BaseOEAModule):\r\n",
					"    def __init__(self, source_folder, pseudonymize = True):\r\n",
					"        BaseOEAModule.__init__(self, source_folder, pseudonymize)\r\n",
					"        \r\n",
					"    def createDictionary(self):\r\n",
					"        file_list = mssparkutils.fs.ls(self.stage1np)\r\n",
					"        for file in file_list:\r\n",
					"            # ignore files\r\n",
					"            if file.isFile:\r\n",
					"                continue\r\n",
					"            \r\n",
					"            tokens = file.path.split(\"/\")\r\n",
					"            print(tokens[-1])\r\n",
					"\r\n",
					"dictionary = DataDictionary(source_folder)\r\n",
					"\r\n",
					"dictionary.createDictionary()"
