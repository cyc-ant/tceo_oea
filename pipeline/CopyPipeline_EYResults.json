{
	"name": "CopyPipeline_EYResults",
	"properties": {
		"activities": [
			{
				"name": "ForEach_lot",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_lot",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.listName}"
								},
								{
									"name": "Destination",
									"value": "stage1np/EarlyYearsTestResults/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "SharePointOnlineListSource",
									"httpRequestTimeout": "00:05:00"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_EYResults",
									"type": "DatasetReference",
									"parameters": {
										"cw_listName": "@item().source.listName"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_EYResults",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"listName": "PrepTestQuestions"
						},
						"destination": {
							"fileName": "PrepTestQuestions.csv"
						}
					},
					{
						"source": {
							"listName": "PrepTestResultPost"
						},
						"destination": {
							"fileName": "PrepTestResultPost.csv"
						}
					},
					{
						"source": {
							"listName": "PrepTestResultPre"
						},
						"destination": {
							"fileName": "PrepTestResultPre.csv"
						}
					},
					{
						"source": {
							"listName": "Year1TestQuestions"
						},
						"destination": {
							"fileName": "Year1TestQuestions.csv"
						}
					},
					{
						"source": {
							"listName": "Year1TestResultPost"
						},
						"destination": {
							"fileName": "Year1TestResultPost.csv"
						}
					},
					{
						"source": {
							"listName": "Year1TestResultPre"
						},
						"destination": {
							"fileName": "Year1TestResultPre.csv"
						}
					},
					{
						"source": {
							"listName": "Year2TestQuestions"
						},
						"destination": {
							"fileName": "Year2TestQuestions.csv"
						}
					},
					{
						"source": {
							"listName": "Year2TestResultPost"
						},
						"destination": {
							"fileName": "Year2TestResultPost.csv"
						}
					},
					{
						"source": {
							"listName": "Year2TestResultPre"
						},
						"destination": {
							"fileName": "Year2TestResultPre.csv"
						}
					}
				]
			}
		},
		"annotations": []
	}
}