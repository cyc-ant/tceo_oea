{
	"name": "CopyPipeline_ODSStudentDemo",
	"properties": {
		"activities": [
			{
				"name": "Copy_zxg",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "."
					},
					{
						"name": "Destination",
						"value": "stage1np/_ODSStudentDemographic/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "select\n    SKStudent\n    , CompassPersonId \n    , FirstName\n    , LastName\n    , GovCode1\n    , IsCurrent\nfrom Compass.ODSStudentDemographic",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": "CompassODSStudentDemographic.csv"
						}
					},
					"enableStaging": false,
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_ODSStudentDemo",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_ODSStudentDemo",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}