					"ifTrueActivities": [
						{
							"name": "Create_SQLDB",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "Create_SQLDB_nohashing",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true
							}
